struct Vec3{
    x: float;
    y: float;
    z: float;
}
table Cube{
    /// position of cube
    pos: Vec3;        
    /// acceleration of cube
    accel: Vec3;  
    /// velocity of cube
    vel: Vec3;    
    /// mass of cube
    mass: float;    
}

table Cubes{
    cubes: [Cube];
}

root_type Cubes;
